package com.baseball;
import java.util.Scanner;

public class BattingAverage {
//create method to analyze hits and walks vs ABs.

	public int hits;
	public int atBats;
	public int walks;

	void battingAverage(int hits, int atBats, int walks) {
		this.hits = hits;
		this.atBats = atBats;
		this.walks = walks;

	}
	
}


